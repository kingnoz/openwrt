+/dts-v1/;
+#include "hi3536dv100.dtsi"
+
+/ {
+	model = "Hisilicon HI3536DV100 DEMO Board";
+	compatible = "hisilicon,hi3536dv100";
+
+	memory {
+		device_type = "memory";
+		reg = <0x80000000 0x20000000>;
+	};
+};
+
+&uart0 {
+	status = "okay";
+};
+
+&i2c_bus0 {
+    status = "okay";
+    clock-frequency = <100000>;
+};
+
+&dual_timer0 {
+	status = "okay";
+};
+
